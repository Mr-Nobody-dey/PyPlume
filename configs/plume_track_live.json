    "repeat_dt": 3600,
    "repetitions": -1,
    "instances_per_spawn": 1,
    "spawn_points": "data/spawn_points_mats/glist90zj_pts_position_spawn.mat",
    "simulation_dt": 300,
    "snapshot_interval": 3600
  },
  "postprocess_config": {},
  "save_snapshots": true,
  "plotting_config": {
    "shown_domain": {
      "S": 32.525,
      "N": 32.7,
      "W": -117.27,
      "E": -117.09
    },
    "save_dir_snapshots": "snapshots",
    "plot_feature_set": "tj_plume_tracker",
    "draw_coasts": false
  }
}
